print(path)

path_to_data='/workspaces/dplPy/tests/data'

data_files = glob.glob(path_to_data+'*.txt')

for file in data_files:
    print(file)
    with open(file) as f:
        content = f.readlines()
        content = [x.strip() for x in content]
        content = [x.split('\t') for x in content]
        content = [x[0] for x in content]
        content = [x.split('-') for x in content]
        content = [x[0] for x in content]
        content = [x.split(' ') for x in content]
        content = [x[0] for x in content]
        content = [x.split('/') for x in content]
        content = [x[0] for x in content]
        content = [x.split('.') for x in content]
        content = [x[0] for x in content]
        content = [x.split(',') for x in content]
        content = [x[0] for x in content]
        content = [x.split('_') for x in content]
        content = [x[0] for x in content]
        content = [x.split('(') for x in content]
        content = [x[0] for x in content]
        content = [x.split(')') for x in content]
        content = [x[0] for x in content]
        content = [x.split(':') for x in content]
        content = [x[0] for x in content]
        content = [x.split(';') for x in content]
        content = [x[0] for x in content]

